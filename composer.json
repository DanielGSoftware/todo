{
  "name": "danielg/todo",
  "description": "A simple todo application, decoupled from infrastructure",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "DanielG",
      "email": "danielgsoftware@gmail.com"
    }
  ],
  "autoload": {
    "classmap": [
      "src/"
    ],
